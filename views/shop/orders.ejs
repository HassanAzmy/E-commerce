<!DOCTYPE html>
<html lang="en">

<head>
   <%- include('../includes/head') %>
</head>

<body>
   <%- include('../includes/navBar') %>
   <main>
      <%if(orders.length > 0) {%>
         <ul>
            <%orders.forEach(order => {%>
               <li>
                  <h1># <%=order.Id%></h1>
                  <ul>
                     <%order.products.forEach(product => {%>
                        <li><%=product.title%> (<%=product.orderItem.quantity%>)</li>
                     <%});%>
                  </ul>
               </li>
            <%});%>
         </ul>
      <%} else {%>
         <h1>Nothing There!</h1>
      <%}%>
   </main>
</body>

</html>